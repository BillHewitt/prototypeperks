<%- content_for :head_css do %>
  
<%= stylesheet_link_tag 'admin/libs/jquery-nice-select/nice-select.css', media: 'all', 'data-turbolinks-track': 'reload' %>

<%= stylesheet_link_tag 'admin/libs/switchery/switchery.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>

<%= stylesheet_link_tag 'admin/libs/multiselect/multi-select.css', media: 'all', 'data-turbolinks-track': 'reload' %>

<%= stylesheet_link_tag 'admin/libs/select2/select2.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>

<%= stylesheet_link_tag 'admin/libs/bootstrap-select/bootstrap-select.min.css', media: 'all', 'data-turbolinks-track': 'reload' %>

<%= stylesheet_link_tag 'admin/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.css', media: 'all', 'data-turbolinks-track': 'reload' %>

<% end %>

<div class="wrapper">
            <div class="container">

                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box">
                            <h4 class="page-title">Search Results</h4>
                        </div>
                    </div>
                </div>
                <!-- end page title -->

                
                <div class="row">
                    <div class="col-lg-12 col-xl-12">

                        <div class="search-result-box m-t-30 card-box">
                            <div class="row">
                                <div class="col-md-8 offset-md-2">
                                    <div class="pt-3 pb-4">
                                        <div class="input-group m-t-10">
                                            <input type="text" class="form-control" value="Coffee">
                                        <span class="input-group-append">
                                            <button type="button" class="btn btn-light"><i class="fa fa-search mr-1"></i> Search</button>
                                        </span>
                                        </div>
                                        <div class="mt-3 text-center">
                                            <h4>Search Results For "Coffee"</h4>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- end row -->

                            <ul class="nav nav-tabs nav-bordered">
                                <li class="nav-item">
                                    <a href="#home" data-toggle="tab" aria-expanded="true" class="nav-link active">
                                        All results <span class="badge badge-light ml-1">2</span>
                                    </a>
                                </li>
                                
                            </ul>

                            <div class="tab-content">
                                <div class="tab-pane active" id="home">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="search-item">
                                                <h4 class="mb-1"><a href="#">Cafe Nero</a></h4>
                                                <div class="font-13 mb-2 text-truncate">
                                                    10% Discount Code
                                                </div>
                                            </div> <!-- end search item -->

                                            <div class="search-item">
                                                <h4 class="mb-1"><a href="#">Starbucks</a></h4>
                                                <div class="font-13 mb-2 text-truncate">
                                                    5% Gift Card
                                                </div>
                                            </div> <!-- end search item -->

                                            
                                            </div> <!-- end search item -->


                                            

                                            <div class="clearfix"></div>
                                        </div>
                                    </div>
                                </div>

                                <!-- end All results tab -->


                                

                            </div>
                        </div>
                        
                    </div>
                    
                </div>
                
            </div> <!-- end container -->
       </div><%- content_for :footer_js do %>
  
<%= javascript_include_tag 'admin/libs/jquery-nice-select/jquery.nice-select.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/switchery/switchery.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/multiselect/jquery.multi-select.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/select2/select2.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/jquery-mockjax/jquery.mockjax.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/autocomplete/jquery.autocomplete.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/bootstrap-select/bootstrap-select.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/bootstrap-touchspin/jquery.bootstrap-touchspin.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/libs/bootstrap-maxlength/bootstrap-maxlength.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'admin/js/pages/form-advanced.init.js', 'data-turbolinks-track': 'reload' %>
<% end %>